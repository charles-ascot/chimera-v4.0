# CHIMERA v4 - Backend Environment Variables
# Copy to .env and fill in values

# Application
APP_NAME=CHIMERA v4
DEBUG=false
ENVIRONMENT=development
LOG_LEVEL=INFO

# Google Cloud
GCP_PROJECT_ID=chimera-v4
GCP_PROJECT_NUMBER=950990732577
GCS_BUCKET_NAME=chimera-v4-data
GCS_MODELS_BUCKET=chimera-v4-models

# BigQuery
BQ_DATASET=horse_racing
BQ_RACES_TABLE=races
BQ_PREDICTIONS_TABLE=predictions
BQ_TRAINING_DATA_TABLE=training_data

# ML Settings
MODEL_VERSION=1.0.0
SMOTE_K_NEIGHBORS=5
RANDOM_FOREST_N_ESTIMATORS=100
CROSS_VALIDATION_FOLDS=5
TRAIN_TEST_SPLIT=0.7

# AI/LLM (Optional - for future agent enhancements)
OPENAI_API_KEY=
ANTHROPIC_API_KEY=

# Security
SECRET_KEY=change-this-in-production
